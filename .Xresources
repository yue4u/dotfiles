!--------------------xdefauls
URxvt.font: xft:Fira Mono:size=14,style=medium
URxvt.boldFont: xft:Fira Mono:size=14,style=bold
urxvt*termName: rxvt
urxvt*scrollBar: false 
urxvt*matcher.button: 1
URxvt.letterSpace: -1
Xft*dpi: 96
Xft*antialias: true
Xft*hinting: true
Xft*hintstyle: hintfull
URxvt*cursorUnderline: true
Xft*rgba: rgb
URxvt*geometry: 85x20
*internalBorder: 26
URxvt*fading: 30
URxvt*tint: #000000
URxvt*inheritPixmap: False
! special
URxvt*depth: 32
*.foreground:   #c5c8c6
*.background:   #1f1f1f 
*.cursorColor:  #c5c8c6

! black
*.color0:       #282a2e
*.color8:       #373b41

! red
*.color1:       #a85377
*.color9:       #a85377

! green
*.color2:       #746f81
*.color10:      #746f81

! yellow
*.color3:       #545670
*.color11:      #545670

! blue
*.color4:       #6a9bb9
*.color12:      #6a9bb9

! magenta
*.color5:       #764970
*.color13:      #764970

! cyan
*.color6:       #abb9ba
*.color14:      #abb9ba

! white
*.color7:       #899298
*.color15:      #899298

URxvt*transparent: true
URxvt*shading: 30
